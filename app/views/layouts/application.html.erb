<!DOCTYPE html>
<html>
  <head>
    <title>TestTripManager</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  	 <% if user_signed_in? %>

		  <nav class="navbar navbar-default">
			  <div class="container-fluid">
			    <!-- Brand and toggle get grouped for better mobile display -->
			    <div class="navbar-header">
			      
			      <a class="navbar-brand" href="#">Test Trip Manager</a>
			    </div>

			    <!-- Collect the nav links, forms, and other content for toggling -->
			    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
			      <ul class="nav navbar-nav">
			        <li class="active"><a href="<%= root_path %>">Home</a></li>
			        <li><a href="<%= vehicles_path %>">Vehicles</a></li>
			        <li><a href="<%= passengers_path %>">Passenger</a></li>
			        <li><a href="<%= travels_path %>">Recorridos</a></li>
			      </ul>
			      
			      
			    </div><!-- /.navbar-collapse -->
			  </div><!-- /.container-fluid -->
			</nav>
		<% end %>
	  <div class="container">
	    <%= yield %>
	  </div>
  </body>
</html>
